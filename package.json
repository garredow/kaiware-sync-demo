{
  "name": "kaiware-sync-demo",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "npm run clean && export BROWSERSLIST_ENV=kaios2 && parcel build ./src/**/*.html --no-source-maps --no-cache --target kaios2",
    "clean": "rimraf ./dist",
    "postversion": "./deploy/version.sh $npm_package_version",
    "commit": "cz"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "kaiware-sync": "^2.0.1"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.3.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "parcel": "^2.3.1",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "prettier": "2.6.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2"
  },
  "staticFiles": {
    "staticPath": "public"
  },
  "browserslist": {
    "kaios2": "Firefox 48",
    "kaios3": "Firefox 84"
  },
  "targets": {
    "kaios2": {
      "distDir": "./dist"
    },
    "kaios3": {
      "source": "src/index.html",
      "distDir": "./dist"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
